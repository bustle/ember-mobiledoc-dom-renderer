{
  "name": "ember-mobiledoc-dom-renderer",
  "version": "1.0.0-1",
  "description": "provides a runtime mobiledoc dom-renderer to ember apps",
  "keywords": [
    "ember-addon",
    "mobiledoc",
    "mobiledoc-kit"
  ],
  "repository": "https://github.com/bustlelabs/ember-mobiledoc-dom-renderer",
  "license": "MIT",
  "author": "Bustlelabs",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "format": "prettier --write .",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:types": "glint",
    "release": "release-it",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "@embroider/util": "^1.13.4",
    "@glimmer/component": "^1.1.2",
    "broccoli-funnel": "^3.0.8",
    "broccoli-merge-trees": "^4.2.0",
    "ember-auto-import": "^2.11.1",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-cli-typescript": "^5.3.0",
    "ember-in-element-polyfill": "^1.0.1",
    "ember-template-imports": "^4.3.0",
    "mobiledoc-dom-renderer": "^0.7.2"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.28.4",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.28.0",
    "@ember/optional-features": "^2.2.0",
    "@ember/test-helpers": "^5.3.0",
    "@embroider/test-setup": "^4.0.0",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.10.0",
    "@glimmer/tracking": "^1.1.2",
    "@glint/core": "^1.5.2",
    "@glint/environment-ember-loose": "^1.5.2",
    "@glint/environment-ember-template-imports": "^1.5.2",
    "@glint/template": "^1.6.2",
    "@release-it-plugins/lerna-changelog": "^8.0.1",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^2.11.1",
    "ember-cli": "~5.12.0",
    "ember-cli-dependency-checker": "^3.3.3",
    "ember-cli-fastboot": "^4.1.5",
    "ember-cli-fastboot-testing": "^0.6.2",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-eslint-parser": "^0.5.11",
    "ember-load-initializers": "^3.0.1",
    "ember-maybe-import-regenerator": "^1.0.0",
    "ember-page-title": "^9.0.3",
    "ember-qunit": "^9.0.4",
    "ember-resolver": "^13.1.1",
    "ember-source": "~5.12.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^7.9.3",
    "ember-try": "^4.0.0",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-ember": "^12.7.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-qunit": "^8.2.5",
    "fastboot": "^4.1.5",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "prettier-plugin-ember-template-tag": "^2.1.0",
    "qunit": "^2.24.2",
    "qunit-dom": "^3.5.0",
    "release-it": "^19.0.5",
    "typescript": "^5.6.3",
    "webpack": "5.102.1"
  },
  "engines": {
    "node": "20.* || >= 22"
  },
  "ember": {
    "edition": "octane"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "release-it": {
    "plugins": {
      "@release-it-plugins/lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": true
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  },
  "volta": {
    "node": "20.19.1",
    "pnpm": "10.12.1"
  },
  "pnpm": {
    "overrides": {
      "fastboot": "4.1.5",
      "fastboot-express-middleware": "4.1.2"
    }
  }
}
